{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/types.*$",
    "^@/constants.*$",
    "^@/context.*$",
    "^@/hooks.*$",
    "^@/utils.*$",
    "^@/features.*$",
    "^@/components.*$",
    "^@/assets.*$",
    "^[./]",
    "^.*.css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
